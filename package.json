{
  "name": "remix-template-remix-ts",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "dev": "concurrently \"remix dev\" \"npm run dev:css\" \"npm run json-server\"",
    "dev:css": "tailwindcss -o ./app/styles/tailwind.css --watch",
    "postinstall": "remix setup node",
    "start": "remix-serve build",
    "json-server": "json-server --watch db.json --port 3001"
  },
  "dependencies": {
    "@remix-run/react": "^1.3.3",
    "@remix-run/serve": "^1.3.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.3.3"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.3.3",
    "@remix-run/eslint-config": "^1.3.3",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "concurrently": "^7.0.0",
    "eslint": "^8.11.0",
    "json-server": "^0.17.0",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.5.5"
  },
  "engines": {
    "node": ">=14"
  }
}
